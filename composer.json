{
    "name": "siebsie23/laravel-mailbox",
    "description": "Handle incoming emails in your Laravel application.",
    "keywords": [
        "siebsie23",
        "laravel-mailbox"
    ],
    "homepage": "https://github.com/siebsie23/laravel-mailbox",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "illuminate/container": "^9.0|^10.0",
        "illuminate/database": "^9.0|^10.0",
        "illuminate/log": "^9.0|^10.0",
        "illuminate/routing": "^9.0|^10.0",
        "illuminate/support": "^9.0|^10.0",
        "willdurand/email-reply-parser": "^2.10",
        "zbateson/mail-mime-parser": "^2.4"
    },
    "require-dev": {
        "laminas/laminas-mail": "^2.25",
        "mockery/mockery": "^1.6",
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "BeyondCode\\Mailbox\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BeyondCode\\Mailbox\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "BeyondCode\\Mailbox\\MailboxServiceProvider"
            ],
            "aliases": {
                "Mailbox": "BeyondCode\\Mailbox\\Facades\\Mailbox"
            }
        }
    }
}
